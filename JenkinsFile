pipeline {
    agent any
    triggers {
        pollSCM('* * * * *')
    }

    stages {
        stage('GIT-Checkout'){
            steps{
                script{
                    sh'''
                        curl -u https://github.com/RhitamB-Eximietas/TestingFramework.git
                    '''
                }
            }
        }
        stage('Test') {
            steps{
                script{
                    sh '''
                        cd custom-jenkins
                        docker build -t myjenkins-blueocean:2.452.1-1 .
                        
                    '''
                }
            }
        }
    }
}
