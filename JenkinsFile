pipeline {
    agent any
    triggers {
        pollSCM('* * * * *')
    }

    stages {
        stage('GIT-Checkout'){
            steps{
                script{
                    sh'''
                        git clone https://github.com/RhitamB-Eximietas/TestingFramework.git
                    '''
                }
            }
        }
        stage('Test') {
            steps{
                script{
                    sh 'docker build -t myjenkins-blueocean:2.452.1-1 .'
                }
            }
        }
    }
}
